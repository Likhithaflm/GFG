class Solution{
public:	
		
	string removeDups(string S) 
	{
	    string t;
	    for(int i=0; i<S.size(); i++)
	    {
	        int j;
	        for(j=0; j<i; j++)
	        {
	            if (S[i] == S[j])
                break;
	        }
	        if(i==j)
	        t=t+S[i];
	        
	    }
	    return t;
	}
